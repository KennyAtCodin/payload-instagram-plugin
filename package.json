{
  "name": "payload-instagram-plugin",
  "version": "0.1.0-beta.2",
  "type": "module",
  "homepage:": "https://github.com/tomashco/payload-instagram-plugin",
  "repository": "git@github.com:tomashco/payload-instagram-plugin.git",
  "description": "Instagram plugin for Payload CMS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "typescript",
    "react",
    "Instagram"
  ],
  "scripts": {
    "dev": "cd dev && cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_DROP_DATABASE='true' next dev --turbo --experimental-https",
    "build": "cd dev && cross-env NODE_OPTIONS=--no-deprecation next build",
    "test": "cd test && jest --config=./jest.config.js",
    "test:e2e": "cross-env NODE_OPTIONS=--no-deprecation NODE_NO_WARNINGS=1 tsx ./test/runE2E.ts",
    "lint": "eslint src",
    "payload": "cd dev && cross-env NODE_OPTIONS=--no-deprecation payload",
    "dev:lint": "cd dev && cross-env NODE_OPTIONS=--no-deprecation next lint",
    "generate:types": "cd dev && payload generate:types",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "author": "tomashco@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@payloadcms/db-mongodb": "v3.23.0",
    "@payloadcms/eslint-config": "^3",
    "@payloadcms/next": "v3.23.0",
    "@payloadcms/richtext-lexical": "v3.23.0",
    "@payloadcms/ui": "v3.23.0",
    "@playwright/test": "1.50.1",
    "@swc/jest": "^0.2.37",
    "@tanstack/react-query": "^5.66.3",
    "@tanstack/react-query-devtools": "^5.66.3",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.4",
    "@types/react": "19.0.9",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.24.0",
    "comment-json": "^4.2.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "glob": "^11.0.1",
    "jest": "^29.7.0",
    "minimist": "^1.2.8",
    "mongodb-memory-server": "^10.1.3",
    "next": "15.1.7",
    "payload": "v3.23.0",
    "prettier": "^3.5.1",
    "qs": "^6.14.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-player": "^2.16.0",
    "sharp": "^0.33.5",
    "shelljs": "^0.8.5",
    "slash": "^5.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "mongodb-memory-server",
      "sharp"
    ]
  }
}
